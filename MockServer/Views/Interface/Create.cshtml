@model ApiInterfaceCreateModel
@{
    ViewData["Title"] = "创建API接口";
}

<div class="row">
    <div class="col">
        <form asp-action="Create">
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                <div class="col">

                    <div class="form-group">
                        <label asp-for="Category" class="control-label">Category</label>
                        <input asp-for="Category" class="form-control" />
                        <span asp-validation-for="Category" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="RequestPath" class="control-label">RequestPath</label>
                        <input asp-for="RequestPath" class="form-control" />
                        <span asp-validation-for="RequestPath" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ResponseResult" class="control-label">返回结果</label>
                        <textarea asp-for="ResponseResult" class="form-control" rows="12"></textarea>
                        <span asp-validation-for="ResponseResult" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Remark" class="control-label">Remark</label>
                        <textarea asp-for="Remark" class="form-control" rows="12"></textarea>
                        <span asp-validation-for="Remark" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


